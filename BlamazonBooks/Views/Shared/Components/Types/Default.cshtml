
@model IEnumerable<string>

<!--Link to Home-->
<a asp-controller="Home" asp-action="Index"asp-route-Category="@Model">Home</a>

<!--Dynamically create links for each category-->
@foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-Category="@t">@t</a>
    }
